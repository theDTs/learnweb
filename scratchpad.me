package main

import (
	"github.com/gopherjs/gopherjs/js"
)

func main() {
    doc := js.Global.Get("document")
    body := doc.Get("body")
	title := doc.Call("createElement", "H1")
	text := doc.Call("createTextNode", "This is only a text.")
	title.Call("appendChild", text)
	body.Set("innerHTML", "")
	body.Call("appendChild", title)
}